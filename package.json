{
  "name": "My_Project_Here",
  "version": "0.1.1",
  "private": true,
  "description": "Description of project here",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.graphql 'client/src/**/*' 'server/src/**/*' && tsc --noEmit",
    "setup": "npm run server:src:build && ./scripts/setup",
    "server:src:build": "npm run codegen && cd server && tsc",
    "server:src:watch": "cd server && tsc --watch",
    "server:watch": ". ./scripts/env && nodemon --watch 'server/dist/server/**/*.js' -x 'node -r source-map-support/register' server/dist/server/index.js",
    "server:worker:watch": ". ./scripts/env && cd server/dist/worker && graphile-worker --watch",
    "db:watch": ". ./scripts/env && graphile-migrate watch",
    "db:migrate": ". ./scripts/env && graphile-migrate",
    "db:reset:test": "./scripts/env && export DATABASE_URL=\"$TEST_DATABASE_URL\" && graphile-migrate reset && graphile-migrate watch --once",
    "dev": "npm run server:src:build && concurrently --kill-others --names 'db,server:src,server:run,worker,codegen' 'npm run db:watch' 'npm run server:src:watch' 'sleep 8; npm run server:watch' 'sleep 5; npm run server:worker:watch' 'npm run codegen:watch'",
    "test": ". ./scripts/env && npm run db:reset:test && jest",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "graphql-codegen --config codegen.yml --watch",
    "licenses": "yarn --silent licenses generate-disclaimer > LICENSES.md",
    "client:src:build": "cd client/src && next build",
    "build": "npm run codegen && npm run client:src:build && npm run server:src:build"
  },
  "author": "Benjie Gillam <code@benjiegillam.com>",
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "@graphile-contrib/pg-simplify-inflector": "^5.0.0-beta.1",
    "@graphile/pg-pubsub": "^4.4.0",
    "@graphile/subscriptions-lds": "^4.4.0",
    "@graphql-codegen/add": "^1.2.0",
    "@types/chalk": "^2.2.0",
    "@types/connect-redis": "^0.0.9",
    "@types/express-session": "^1.15.12",
    "@types/html-to-text": "^1.4.31",
    "@types/lodash": "^4.14.123",
    "@types/mjml": "^4.0.2",
    "@types/morgan": "^1.7.35",
    "@types/next": "^8.0.3",
    "@types/nodemailer": "^4.6.7",
    "@types/nprogress": "^0.0.29",
    "@types/passport": "^1.0.0",
